\par \rtlch\ltrch 
\par \rtlch\ltrch \page {\rtlch \af3\afs16\up6 \ltrch \fs16\up6 #{\footnote \ltrpar \pard\plain \s245 \rtlch\af3\afs20 \ltrch\fs20\lang1033 \ltrpar {\rtlch \af3\afs16\up6 \ltrch \fs16\up6 #\rtlch\ltrch } REFP_COMMENTS}\rtlch\ltrch } {\rtlch \af3 \ltrch 
